    "version": "0.2.0",
    "configurations": [
      
        {
            "name": "Debug Windows",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}\\cmd\\web",
            "args": [
                "-dns=root:_local_root_@tcp(localhost:3306)/breeders?parseTime=true&tls=false",
                "-templatePath=../../templates",
                "-cache=false",
                "-staticResourcesPath=../../static/"
            ],
        },
        {
            "name": "Debug Linux",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/web",
            "args": ["-templatePath=../../templates", "-cache=false", "-staticResourcesPath=../../static/"],
        }
    ]
}